@model IEnumerable<TMBF.Models.Country>

@{
    ViewBag.Title = "Index";
}

<h2>Country Codes</h2>

<table class="table">
    <tr>
        <th>
            Country
        </th>
        <th>
            Code
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ID)
        </td>
        <td>           
            @Html.ActionLink("Details", "Details", new { id=item.ID })
        </td>
    </tr>
}

</table>
